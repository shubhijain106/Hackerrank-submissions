# Enter your code here. Read input from STDIN. Print output to STDOUT

x=int(raw_input())
y=int(raw_input())
z=int(raw_input())
n=int(raw_input())
res=[]
for x in range(0,x+1):
    for y in range(0,y+1):
        for z in range(0,z+1):
            if not(x+y+z==n):
                l=[x,y,z]
                res=res+[l]
print res